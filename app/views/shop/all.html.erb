<% provide(:title, "All") %>

<div class="center jumbotron">
<h1>Products</h1>
<div class="container">
 <div class="row" >
    <% @products.each do |product| %>
      <div class="col-xs-6 col-sm-3">
        <%= link_to image_tag(product.image1), product %>
            <p><%= product.name %></p>
            <p>$<%= product.price %></p>
        </div>
  <% end %>
</div>
</div>
</div>